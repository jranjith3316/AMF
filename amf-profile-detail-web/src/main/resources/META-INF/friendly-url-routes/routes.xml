<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 7.4.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_7_4_0.dtd">
<routes>

	 <route>
		<pattern></pattern>
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>  
        <implicit-parameter name="mvcRenderCommandName">/profile/view</implicit-parameter>                
	</route>
    
   
    <route>
		<pattern>/{tag}/{screenName}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/profile/view</implicit-parameter>                
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>        
        
	</route>
	
	<route>
		<pattern>/{tag}/{screenName}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/profile/edit</implicit-parameter>                
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
        
	</route>
	
	<route>
		<pattern>/{tag}/{screenName}</pattern>
		<implicit-parameter name="mvcRenderCommandName">/profile/privacy</implicit-parameter>                
        <implicit-parameter name="p_p_lifecycle">0</implicit-parameter>
        <implicit-parameter name="p_p_state">normal</implicit-parameter>
        
	</route>
    
</routes>